{
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "yarn run test:mocha",
    "test:mocha": "cross-env NODE_ENV=test mocha",
    "test:mocha:watch": "cross-env NODE_ENV=test mocha --watch",
    "start": "cross-env NODE_ENV=production pm2 start server.js --name='shift'",
    "start:watch": "cross-env NODE_ENV=production pm2 start server.js --name='shift' --watch",
    "start:dev": "cross-env NODE_ENV=development pm2 start server.js --name='shift'",
    "start:dev:watch": "cross-env NODE_ENV=development pm2 start server.js --name='shift' --watch",
    "stop": "pm2 stop 'shift'",
    "restart": "pm2 restart 'shift'",
    "show": "pm2 show 'shift'",
    "build": "cross-env NODE_ENV=production webpack",
    "build:watch": "cross-env NODE_ENV=production webpack --watch",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:dev:watch": "cross-env NODE_ENV=development webpack --watch",
    "init": "node ./utils/initializer.js"
  },
  "name": "shift",
  "version": "0.0.1",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "i18n": "^0.8.3",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pm2": "^2.2.3",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.0",
    "reactable": "^0.14.1",
    "superagent": "^3.3.2",
    "toastr": "^2.1.2",
    "uuid": "^3.0.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^3.2.0",
    "progress-bar-webpack-plugin": "^1.9.2",
    "webpack": "^1.14.0"
  },
  "description": "Simple utility application for dieticians",
